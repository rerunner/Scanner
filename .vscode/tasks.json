{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-std=c++20",
                "-DSTRING_NAMES=1",
                "-DL1D_CACHE_LINE_SIZE=64",
                "-I/usr/local/include",
                "-I/usr/include/hiberlite",
                "-I${workspaceFolder}/src",
                "-I${workspaceFolder}/src/Measure/Leveling",
                "-I${workspaceFolder}/src/MachineControl",
                "-I${workspaceFolder}/include",
                "${workspaceFolder}/src/main.cpp",
                "${workspaceFolder}/src/MachineControl/application/MachineControl.cpp",
                "-o",
                "${workspaceFolder}/test.exe",
                "-L/usr/local/lib",
                "-lraft",
                "-lshm",
                "-lrt",
                "-laffinity",
                "-ldemangle",
                "-lcmdargs",
                "-lboost_system",
                "-lhiberlite"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        },
    ],
    "version": "2.0.0"
}